{
  "rules": [{
    "selector": ".list-group",
    "is": "ul",
    "not": {
      "direct": ["li:not(.list-group-item)"]
    }
  }, {
    "selector": ".input-group",
    "is": "div",
    "direct": ["input"]
  }, {
    "selector": ".input-group-prepend",
    "parent": ".input-group"
  }, {
    "selector": "input#email",
    "attr": ["type='email'"]
  }, {
    "selector": "input[type='text']",
    "attr": ["placeholder"]
  }, {
    "selector": "select",
    "not": {
      "contains": [":not(option)"]
    }
  }, {
    "selector": "input",
    "sibling": ["label"]
  }, {
    "selector": "a",
    "attr": ["href"]
  }]
}
